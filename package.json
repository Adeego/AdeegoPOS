{
  "name": "claude",
  "description": "POS App",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run copy-static",
    "copy-static": "cp -r ./public ./out/",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron .",
    "electron-dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "electron-builder": "electron-builder",
    "pack": "next build && electron-builder -w",
    "dist": "next build && electron-builder"
  },
  "build": {
    "appId": "com.adeegoltd.claude",
    "productName": "CLAUDE",
    "files": [
      "electron/**/*",
      "out/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-table": "^8.19.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-serve": "^2.0.0",
    "lucide-react": "^0.424.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6.26.0",
    "realm": "^12.12.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "author": {
    "name": "Abdiaziz Abdullahi",
    "email": "adeegoltd@gmail.com"
  }
}
